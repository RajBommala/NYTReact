  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "mongoose": "^5.2.16",
    "morgan": "^1.9.1",
    "npm": "^6.4.1",
    "path": "^0.12.7"
  }
}
